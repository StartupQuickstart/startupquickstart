{
  "name": "@startupquickstart/server",
  "version": "0.4.31",
  "description": "Express server for rest api",
  "author": "Thomas Boles <thomas@startupquickstart.com> (https://github.com/startupquickstart)",
  "homepage": "./dist",
  "license": "MIT",
  "main": "dist/index.js",
  "bin": {
    "startupquickstart": "./bin/cli.js",
    "startup": "./bin/cli.js"
  },
  "files": [
    "bin",
    "dist",
    "migrations",
    "seeds",
    ".sequelizerc",
    "README.md"
  ],
  "exports": {
    "./api": "./dist/api/index.js",
    "./lib": "./dist/lib/index.js",
    "./passports": "./dist/passports/index.js",
    ".": "./dist/index.js"
  },
  "scripts": {
    "start": "TZ=UTC node ./dist/app.js",
    "dev": "TZ=UTC PORT=3501 nodemon --exec babel-node ./src/app.js",
    "dev:db": "docker-compose up -d postgres",
    "client:dev": "npm start --prefix src/client",
    "compile-email-assets": "node-sass ./src/emails/styles/styles.scss ./src/emails/dist/styles.css",
    "compile-email-assets-watch": "node-sass -w ./src/emails/styles/styles.scss ./src/emails/dist/styles.css",
    "clean": "rimraf dist",
    "build:server": "rimraf ./dist && NODE_ENV=production babel ./src  --ignore ./src/client,./src/bin --out-dir dist",
    "build:server:copy": "cp -r ./src/emails/dist ./dist/emails",
    "build": "npm-run-all clean build:server compile-email-assets build:server:copy",
    "create-migration": "npx sequelize-cli migration:generate --name $1",
    "db:migrate:dev": "ENV=dev npx sequelize-cli db:migrate --env default",
    "db:migrate:prod": "ENV=prod npx sequelize-cli db:migrate --env default",
    "db:seed:dev": "ENV=dev npx sequelize-cli db:seed:all --env default",
    "db:seed:prod": "ENV=prod npx sequelize-cli db:seed:all --env default"
  },
  "nodemonConfig": {
    "verbose": true,
    "ignore": [
      "./src/client",
      "./src/bin",
      "package-lock.json"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.17.3",
    "@babel/core": "^7.17.5",
    "@babel/node": "^7.14.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.14.7",
    "@babel/register": "^7.17.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-root-import": "^6.6.0",
    "eslint": "^8.9.0",
    "eslint-config-babel": "^9.0.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.11",
    "npm-run-all": "^4.1.5",
    "prompt": "^1.2.2"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.54.1",
    "@aws-sdk/client-ssm": "^3.54.1",
    "@aws-sdk/credential-provider-sso": "^3.54.1",
    "@babel/preset-react": "^7.12.13",
    "@babel/runtime": "^7.17.2",
    "aws-sdk": "^2.1076.0",
    "axios": "^0.26.0",
    "body-parser": "^1.19.2",
    "capitalize": "^2.0.3",
    "classnames": "^2.2.6",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "formidable": "^2.0.1",
    "generate-password": "^1.6.0",
    "glob": "^7.1.6",
    "googleapis": "^95.0.0",
    "juice": "^8.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "node-sass": "^7.0.1",
    "nodemailer": "^6.6.2",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3",
    "pluralize": "^8.0.0",
    "randomatic": "^3.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-html-email": "^3.0.0",
    "rimraf": "^3.0.2",
    "sequelize": "^6.16.1",
    "shelljs": "^0.8.5",
    "stripe": "^8.203.0",
    "uuid": "^8.3.2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "gitHead": "47fcf070cebea64ca69c3e8b6f894da03206ae78"
}
