{
  "name": "@startupquickstart/server",
  "version": "0.2.31",
  "description": "Express server for rest api",
  "author": "Thomas Boles <thomas@startupquickstart.com> (https://github.com/startupquickstart)",
  "homepage": "./dist",
  "license": "MIT",
  "main": "dist/server.js",
  "files": [
    "dist/lib",
    "dist/api",
    "dist/server.js",
    "README.md"
  ],
  "exports": {
    "./api": "./dist/api",
    "./emails": "./dist/emails",
    "./lib": "./dist/lib",
    "./passports": "./dist/passports",
    ".": "./dist"
  },
  "scripts": {
    "start": "TZ=UTC node ./dist/bootstrap.js",
    "dev": "TZ=UTC PORT=3001 nodemon --exec babel-node ./src/bootstrap.js",
    "dev:db": "docker-compose up -d postgres",
    "client:dev": "npm start --prefix src/admin-client",
    "compile-email-assets": "node-sass ./src/emails/styles/styles.scss ./src/emails/dist/styles.css",
    "compile-email-assets-watch": "node-sass -w ./src/emails/styles/styles.scss ./src/emails/dist/styles.css",
    "clean": "rimraf dist",
    "build:server": "rimraf ./dist && NODE_ENV=production babel ./src  --ignore ./src/admin-client,./src/bin --out-dir dist",
    "build:server:copy": "cp -r ./src/emails ./dist/emails",
    "build": "npm-run-all clean build:server compile-email-assets build:server:copy",
    "create-migration": "npx sequelize-cli migration:generate --name $1",
    "db:migrate:dev": "ENV=dev npx sequelize-cli db:migrate --env default",
    "db:migrate:prod": "ENV=prod npx sequelize-cli db:migrate --env default",
    "db:seed:dev": "ENV=dev npx sequelize-cli db:seed:all --env default",
    "db:seed:prod": "ENV=prod npx sequelize-cli db:seed:all --env default"
  },
  "nodemonConfig": {
    "verbose": true,
    "ignore": [
      "./src/admin-client",
      "./src/bin",
      "package-lock.json"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.7",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.14.7",
    "@babel/register": "^7.12.13",
    "babel-eslint": "^10.1.0",
    "babel-plugin-root-import": "^6.6.0",
    "commander": "^7.2.0",
    "copy-paste": "^1.3.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.30.0",
    "eslint-config-babel": "^9.0.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.1.4",
    "google-auth-library": "^7.3.0",
    "nodemon": "^2.0.11",
    "npm-run-all": "^4.1.5",
    "prompt": "^1.1.0",
    "sequelize-cli": "^6.2.0",
    "serverless-domain-manager": "^5.1.0",
    "serverless-plugin-warmup": "5.3.1",
    "serverless-pseudo-parameters": "^2.5.0"
  },
  "dependencies": {
    "@babel/preset-react": "^7.12.13",
    "@babel/runtime": "^7.14.6",
    "aws-param-store": "^3.2.0",
    "axios": "^0.21.1",
    "axios-extensions": "^3.1.3",
    "body-parser": "^1.19.0",
    "capitalize": "^2.0.3",
    "cheerio": "^1.0.0-rc.5",
    "classnames": "^2.2.6",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "create-react-app": "^4.0.2",
    "crypto": "^1.0.1",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "formidable": "^1.2.2",
    "generate-password": "^1.6.0",
    "glob": "^7.1.6",
    "googleapis": "^76.0.0",
    "juice": "^8.0.0",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "node-sass": "^6.0.1",
    "nodemailer": "^6.6.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "pluralize": "^8.0.0",
    "randomatic": "^3.1.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-html-email": "^3.0.0",
    "request-promise": "^4.2.6",
    "sequelize": "^6.6.5",
    "sequelize-querystring": "^0.10.0",
    "serverless-api-cloudfront": "^0.9.5",
    "serverless-aws-static-file-handler": "^3.0.1",
    "serverless-http": "^2.7.0",
    "shelljs": "^0.8.4",
    "spellchecker": "^3.7.1",
    "stripe": "^8.161.0",
    "uuid": "^8.3.2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "gitHead": "47fcf070cebea64ca69c3e8b6f894da03206ae78"
}
